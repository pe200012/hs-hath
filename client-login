let ClientProxy =
      { host : Text
      , port : Integer
      , auth : Optional { _1 : Text, _2 : Text }
      }

let ClientConfig =
      { Type =
          { clientId : Text
          , key : Text
          , version : Text
          , proxy : Optional ClientProxy
          , downloadDir : Text
          , cachePath : Text
          }
      , default = { version = "", proxy = None ClientProxy }
      }

in  ClientConfig::{
    , clientId = "46607"
    , key = "xn4FTnm4prvcV56NhjAh"
    , version = "160"
    , downloadDir = "./download"
    , cachePath = "./cache"
    }
